<!DOCTYPE html>

<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width,initial-scale=1" />
	<title>@ViewBag.Title - FineArt</title>
	<link rel="icon" type="image/png" sizes="16x16" href="~/favicon.png">
	<link href="~/lib/i_admin/vendor/select2/css/select2.min.css" rel="stylesheet" />
	<link href="~/css/site.css" rel="stylesheet" asp-append-version="true" />
	<link href="~/lib/i_admin/vendor/toastr/css/toastr.min.css" rel="stylesheet" asp-append-version="true" />

	<link href="~/lib/i_admin/vendor/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css" rel="stylesheet" />
	<link href="~/lib/i_admin/vendor/pickadate/themes/default.css" rel="stylesheet" />
	<link href="~/lib/i_admin/vendor/pickadate/themes/default.date.css" rel="stylesheet" />

	<link href="~/lib/i_admin/vendor/jquery-steps/css/jquery.steps.css" rel="stylesheet" />

	<link href="~/lib/i_admin/css/style.css" rel="stylesheet" asp-append-version="true" />
</head>
<body>
	
	<div id="main-wrapper">
			<div class="container my-4">
				@RenderBody()
			</div>
	</div>
	
	
			

	@*Required Vendors*@
	<script src="~/lib/i_admin/vendor/jquery/jquery.min.js"></script>
	<script src="~/lib/i_admin/vendor/global/global.min.js"></script>
	<script src="~/lib/i_admin/js/quixnav-init.js"></script>
	<script src="~/lib/i_admin/js/custom.min.js"></script>

	<script src="~/lib/i_admin/vendor/jquery-validation/jquery.validate.min.js"></script>
	<script src="~/lib/i_admin/vendor/toastr/js/toastr.min.js"></script>

	<script src="~/lib/i_admin/vendor/moment/moment.min.js"></script>
	<script src="~/lib/i_admin/vendor/bootstrap-daterangepicker/daterangepicker.js"></script>
	<script src="~/lib/i_admin/vendor/clockpicker/js/bootstrap-clockpicker.min.js"></script>
	<script src="~/lib/i_admin/vendor/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>
	<script src="~/lib/i_admin/vendor/pickadate/picker.js"></script>
	<script src="~/lib/i_admin/vendor/pickadate/picker.date.js"></script>
	<script src="~/lib/i_admin/vendor/pickadate/picker.time.js"></script>

	<script src="~/lib/i_admin/vendor/jquery-steps/build/jquery.steps.min.js"></script>
	<script src="~/lib/i_admin/js/plugins-init/jquery-steps-init.js"></script>

	<script src="~/lib/i_admin/js/plugins-init/bs-daterange-picker-init.js"></script>
	<script src="~/lib/i_admin/js/plugins-init/clock-picker-init.js"></script>
	<script src="~/lib/i_admin/js/plugins-init/material-date-picker-init.js"></script>
	<script src="~/lib/i_admin/js/plugins-init/pickadate-init.js"></script>

	<script src="~/lib/i_admin/vendor/select2/js/select2.full.min.js"></script>
	<script src="~/lib/i_admin/js/plugins-init/select2-init.js"></script>
	<script src="~/js/site.js"></script>

	@RenderSection("Scripts", required: false)
</body>
</html>
